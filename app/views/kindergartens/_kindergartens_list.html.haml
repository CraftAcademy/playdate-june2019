%table
  %tr 
    %th name
    %th kommun
    %th address
  - @kindergartens.each do |kindergarten|
    %tr{id: dom_id(kindergarten)}
      %td
        = kindergarten.name
      %td
        = kindergarten.kommun
      %td
        = kindergarten.address
      %td
        = link_to 'Add kindergarten to profile', kindergarten_path(kindergarten), method: :put